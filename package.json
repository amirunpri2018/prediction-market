{
  "name": "prediction-market",
  "version": "0.1.0",
  "description": "FCLA decentralized prediction markets",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build-css": "node-sass-chokidar src/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o public/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts --max_old_space_size=4096 build",
    "build": "npm-run-all clean build-css build-js",
    "compile": "truffle compile --all",
    "migrate": "truffle migrate && cd ./truffle-chainlink/ && truffle migrate",
    "migrate-rinkeby": "truffle migrate --network rinkeby && cd ./truffle-chainlink/ && truffle migrate --network rinkeby",
    "lintjs": "eslint .",
    "fmtjs": "eslint . --fix",
    "test": "truffle test --network test",
    "test-app": "jest"
  },
  "keywords": [
    "Oracle",
    "Ethereum"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@gnosis.pm/hg-contracts": "^0.2.3",
    "@gnosis.pm/hg-market-makers": "^0.3.1",
    "@openzeppelin/contracts": "^2.4.0",
    "@rimble/connection-banner": "^1.2.3",
    "@rimble/network-indicator": "^1.2.3",
    "@rimble/utils": "^1.2.3",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "canonical-weth": "^1.4.0",
    "chainlink": "^0.6.5",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "decimal.js-light": "^2.5.0",
    "drizzle": "^1.4.0",
    "drizzle-react": "^1.3.0",
    "eslint-config-prettier": "^6.0.0",
    "google-map-react": "^1.1.5",
    "hex2dec": "^1.1.2",
    "history": "^4.9.0",
    "inquirer": "^6.3.1",
    "lodash": "^4.17.11",
    "lodash.combinations": "^18.9.19",
    "normalize.css": "^8.0.1",
    "openzeppelin-solidity": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.3",
    "rimble-ui": "^0.10.0",
    "solidity-util": "github:willitscale/solidity-util",
    "styled-components": "^4.4.0",
    "truffle-contract": "^4.0.13",
    "truffle-hdwallet-provider": "^1.0.8",
    "web3": "1.0.0-beta.37"
  },
  "author": "Gnosis",
  "license": "MIT",
  "devDependencies": {
    "@gnosis.pm/truffle-nice-tools": "^1.3.0",
    "@machinomy/types-truffle-contract": "0.0.4",
    "@types/classnames": "^2.2.8",
    "@types/react-redux": "^7.1.1",
    "@types/web3": "^1.0.19",
    "ajv": "^6.10.0",
    "chai": "^4.2.0",
    "common-tags": "^1.8.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^7.0.0",
    "elliptic": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^2.1.0",
    "js-sha3": "^0.8.0",
    "json-x-loader": "^0.3.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "react-scripts": "^3.0.1",
    "rlp": "^2.2.3",
    "run-with-testrpc": "^0.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "truffle": "^5.0.14",
    "truffle-assertions": "^0.8.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "since 2017-06"
  ]
}
